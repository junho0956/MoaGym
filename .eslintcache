ch(() => getBrandList);\r\n}\r\n\r\nfunction getBrandData(brandname:string):Promise<ItemInfoComponent[]>{\r\n    return fetch(`${BACKEND_URL}/api/v1/itemsByBrandWithReview/${brandname}`)\r\n    .then(res => {\r\n        if(res.status === 200) return res.json();\r\n        else throw new Error();\r\n    })\r\n    .then(res => res)\r\n    .catch(() => getBrandData(brandname));\r\n}\r\n\r\nfunction useInitData(){\r\n    const [brandData, setBrandData] = useState<brandDataType[]>([]);\r\n\r\n    const getAllData = useCallback(async() => {\r\n\r\n        let brandList:brandListType[] = await getBrandList();\r\n        \r\n        const brandListData:brandDataType[] = await Promise.all(\r\n            brandList.map(async(brand) => {\r\n                return{\r\n                    brandName: brand.name,\r\n                    brandImg: brand.brandImg,\r\n                    brandId: brand.brandId,\r\n                    brandUrl: brand.brandUrl,\r\n                    brandData: await getBrandData(brand.name),\r\n                }\r\n            })\r\n        );\r\n        \r\n        return brandListData;\r\n    }, [])\r\n\r\n    useEffect(() => {\r\n        const getDataResult = getAllData();\r\n        getDataResult.then(res => {\r\n            setBrandData(res);\r\n        })\r\n    }, []);\r\n\r\n    return brandData;\r\n}\r\n\r\nexport default useInitData;","C:\\Users\\junho\\Desktop\\MoaGym\\dnd-mentee-4th-7-frontend\\src\\common\\getCategoryData.ts",[],"C:\\Users\\junho\\Desktop\\MoaGym\\dnd-mentee-4th-7-frontend\\src\\hook\\useWeekBestItem.ts",[],"C:\\Users\\junho\\Desktop\\MoaGym\\dnd-mentee-4th-7-frontend\\src\\hook\\useBrandList.ts",[],"C:\\Users\\junho\\Desktop\\MoaGym\\dnd-mentee-4th-7-frontend\\src\\hook\\useNewProduct.ts",[],"C:\\Users\\junho\\Desktop\\MoaGym\\dnd-mentee-4th-7-frontend\\src\\hook\\usePhotoReview.ts",[],"C:\\Users\\junho\\Desktop\\MoaGym\\dnd-mentee-4th-7-frontend\\src\\shared\\Search\\state.ts",[],"C:\\Users\\junho\\Desktop\\MoaGym\\dnd-m