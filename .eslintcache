\n\r\nfunction ProductListItem({product}:{product:ItemInfoComponent}){\r\n\r\n  const loadImg = useRef<HTMLImageElement>(document.createElement('img'));\r\n  const [loading, setLoading] = useState(true);\r\n  const item_ref = useRef<HTMLDivElement>(null);\r\n  const move = useRef(true);\r\n  const history = useHistory();\r\n  const price = useRef<string[]>(useCommaNumber(product.productPrice));\r\n  const reviewCnt = useRef<string[]>(useCommaNumber(product.productReviewCnt));\r\n\r\n  useEffect(() => {\r\n    if(item_ref){\r\n      item_ref.current?.addEventListener('mousedown', e => {\r\n        move.current = false;\r\n      })\r\n      item_ref.current?.addEventListener('mousemove', e => {\r\n        move.current = true;\r\n      })\r\n      item_ref.current?.addEventListener('mouseup', e => {\r\n        if(!move.current){\r\n          move.current = true;\r\n          history.push(`/MoaGym/product/${product.productId}`);\r\n          store.dispatch(setProduct(product));\r\n        }\r\n      })\r\n    }\r\n  }, [])\r\n\r\n  useEffect(()=>{\r\n    loadImg.current.className = \"ProductImg\";\r\n    loadImg.current.alt = \"productItem_alt\";\r\n    loadImg.current.onload = function(){\r\n      setLoading(false);\r\n    }\r\n    loadImg.current.src = product.productImageUrl[0].url;\r\n  }, [product]);\r\n\r\n  useEffect(() => {\r\n    if(loading === false){\r\n      (item_ref.current as HTMLDivElement).insertBefore(loadImg.current, (item_ref.current as HTMLDivElement).firstChild);\r\n    }\r\n  }, [loading])\r\n\r\n  return (\r\n    <ProductItem ref={item_ref}>\r\n      {loading ? <ProductLoading /> :\r\n        <React.Fragment>\r\n          {product.productReviewCnt > 99 &&\r\n            (<UpperRight>\r\n              {product.productReviewCnt > 999 ? <img src={badge1000} alt=\"badge\"/> : <img src={badge100} alt=\"badge\"/>}\r\n            </UpperRight>)\r\n          }\r\n          <BrandTitle>{product.brandName}</BrandTitle>\r\n          <ProductTitle>{product.productName}</ProductTitle>\r\n          <ProductPrice>{price.current}원</ProductPrice>\r\n          <img src={ProductReviewStarImg} className=\"ProductReviewStar\" alt=\"pd-img\"/>\r\n          <ProductReviewPoint>{product.productReviewPoint}</ProductReviewPoint>\r\n          <img src={Rectangle} className=\"Rectangle\" alt=\"pd-img\"/>\r\n          <ProductReview>\r\n            <img src={ProductReviewCntImg} alt=\"pdreview-img\"/>\r\n            <ProductReviewCnt>{reviewCnt.current}</ProductReviewCnt>\r\n          </ProductReview>\r\n          <ProductTagWrap>\r\n            <ProductTag>{product.category}</ProductTag>\r\n          </ProductTagWrap>\r\n        </React.Fragment>\r\n      }\r\n    </ProductItem>\r\n  );\r\n}\r\n\r\nexport default React.memo(ProductListItem);","C:\\Users\\junho\\Desktop\\MoaGym\\dnd-mentee-4th-7-frontend\\src\\page\\index.ts",[],"C:\\Users\\junho\\Desktop\\MoaGym\\dnd-mentee-4th-7-frontend\\src\\shared\\index.ts",[],"C:\\Users\\junho\\Desktop\\MoaGym\\dnd-mentee-4th-7-frontend\\src\\shared\\Gnb-component\\state.ts",[],"C:\\Users\\junho\\Desktop\\MoaGym\\dnd-mentee-4th-7-frontend\\src\\shared\\Gnb-component\\GnbMain.tsx",[],"C:\\Users\\junho\\Desktop\\MoaGym\\dnd-mentee-4th-7-frontend\\src\\shared\\Gnb-component\\style.ts",[],"C:\\Users\\junho\\Desktop\\MoaGym\\dnd-mentee-4th-7-frontend\\src\\shared\\Gnb-component\\GnbItem.tsx",[],"C:\\Users\\junho\\Desktop\\MoaGym\\dnd-mentee-4th-7-frontend\\src\\common\\BrandDataReducer.ts",[],"C:\\Users\\junho\\Desktop\\MoaGym\\dnd-mentee-4th-7-frontend\\src\\page\\Brand\\BrandList-page.tsx",[],"C:\\Users\\junho\\Desktop\\MoaGym\\dnd-mentee-4th-7-frontend\\src\\page\\Brand\\style.ts",[],"C:\\Users\\junho\\Desktop\\MoaGym\\dnd-mentee-4th-7-frontend\\src\\page\\Brand\\BrandPage.tsx",["500"],"import React, {useState, useEffect} from 'react';\r\nimport {useSelector} from 'react-redux';\r\nimport {RootState} from '../../common/store';\r\nimport {useLocation} from 'react-router-dom';\r\nimport {\r\n    Bra